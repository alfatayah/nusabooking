<!-- DataTales Example -->
<div class="card shadow mb-4">
    <div class="card-header py-3">
      <!-- Button trigger modal -->
      <a href="/admin/booking" style="color: #FFF; background-color: #F04819;" class="btn btn-warning">Back</a>
    </div>
    <div class="card-body">
      <div class="table-responsive">
        <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
          <thead>
            <tr>
              <th>Product</th>
            </tr>
          </thead>
          <tbody>
            <% for(let i = 0; i < dataProduct.length; i++) { %>
              <tr>
                <td><%= dataProduct[i] %></td>
              </tr>
              <% }%>
          </tbody>
        </table>
      </div>
    </div>
  </div>